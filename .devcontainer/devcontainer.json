{
    "name": "JS ENV",
    "dockerFile": "Dockerfile",
    "appPort": [3000],
    "runArgs": ["-u", "node"],
    "settings": {
        "workbench.colorTheme": "Cobalt2",
        "terminal.integrated.shell.linux": "/bin/bash"
    },
    "postCreateCommand": "npm install",
    "extensions": [
		"esbenp.prettier-vscode",
		"wesbos.theme-cobalt2",
		"rvest.vs-code-prettier-eslint"
	]
}